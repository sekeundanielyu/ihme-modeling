// Use draws generated by custom code and upload to the Epi database
clear all
set more off

adopath + "FILEPATH"

get_best_model_versions, entity(modelable_entity) ids(2413) clear
levelsof model_version_id, local(models)

quietly do FILEPATH/save_results.do

save_results, modelable_entity_id(1455) description("asymp & mild heart failure due to Chagas; GBD2016 splits; best model `models'") in_dir("FILEPATH") metrics(5) mark_best(yes)
save_results, modelable_entity_id(1456) description("moderate heart failure due to Chagas; GBD2016 splits; best model `models'") in_dir("FILEPATH") metrics(5) mark_best(yes)
save_results, modelable_entity_id(1457) description("severe heart failure due to Chagas; GBD2016 splits; best model `models'") in_dir("FILEPATH") metrics(5) mark_best(yes)
